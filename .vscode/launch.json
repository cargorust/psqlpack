{
        "version": "0.2.0",
        "configurations": [
            {
                "name": "(Windows) Launch",
                "type": "cppvsdbg",
                "request": "launch",
                "program": "target/debug/psqlpack.exe",
                "args": [
                    "package",
                    "--source",
                    "samples/complex/project.json",
                    "--out",
                    "out/complex.psqlpack",
                    "--trace"
                ],
                "stopAtEntry": false,
                "cwd": "${workspaceRoot}",
                "environment": [],
                "externalConsole": false,
                "preLaunchTask": "cargo build cli"
            }
        ]
    }
    